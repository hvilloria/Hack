<h1>Contacts#new</h1>

<%= form_for @contact do |f| %>
  <p>
  	<%= f.label :name %>
  	<%= f.text_field :name %>
  </p>
  <p>
  	<%= f.label :mobile %>
  	<%= f.text_field :mobile %>
  </p>
  <p>
  	<%= f.label :phone %>
  	<%= f.text_field :phone %>
  </p>
  <p>
  	<%= f.label :email %>
  	<%= f.text_field :email %>
  </p>
  <p>
  	<%= f.label :email2 %>
  	<%= f.text_field :email2 %>
  </p>
  <p>
  	<%= f.radio_button(:gender, "Masculino", :checked => true) %>
  	<%= f.label(:gender, "Masculino") %><br>
  	<%= f.radio_button(:gender, "Femenino") %>
  	<%= f.label(:gender, "Femenino") %>
  </p>
  <p>
  	<%= f.label :birthdate %>
  	<%= f.text_field :birthdate %>
  </p>
  <p>
    <%= f.label :avatar %>
    <%= f.text_field :avatar %>
  </p>
  <p>
  	<%= f.submit %>
  </p>
<% end %>